<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  
  <link rel="icon" type="image/png" sizes="32x32" href="/images/logo_keos_web.png" />
  <link rel="icon" type="image/png" sizes="16x16" href="/images/logo_keos_web.png" />
  <!-- Boxicons -->
  <link
    href="https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css"
    rel="stylesheet">
  <!-- Custom Stylesheet -->
  <link rel="stylesheet" href="/css/footer.css" />
  <link rel="stylesheet" href="/css/header.css" />
  <title><%= title %></title>

  <% if(state == "index") { %>
  <link rel="stylesheet" href="/css/plugins/glide.core.css" />
  <link rel="stylesheet" href="/css/plugins/glide.theme.css" />
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css" />
  <link rel="stylesheet" href="/css/index.css" />
  <link rel="stylesheet" href="/css/plugins/bootstrap.min.css" />
  <% } else if(state == "Aboutus") { %>
  <link rel="stylesheet" href="/public/css/Aboutus.css">
  <link rel="stylesheet" href="/public/css/plugins/bootstrap.min.css">
  <% } else if(state == "cart") { %>
  <link rel="stylesheet" href="/public/css/cart.css">
  <link rel="stylesheet" href="/public/css/plugins/bootstrap-checkout.min.css">
  <% } else if(state == "contactus") { %>
  <link rel="stylesheet" href="/public/css/contactus.css">
  <link rel="stylesheet" href="/public/css/plugins/bootstrap.min.css">
  <% } else if(state == "FAQ") { %>
  <link rel="stylesheet" href="/public/css/FAQ.css" >
  <link href="https://fonts.googleapis.com/css2?family=Josefin+Sans:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;1,100;1,200;1,300;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="/public/css/plugins/bootstrap.min.css">
  <% } else if(state == "login") { %>
  <link rel="stylesheet" href="/public/css/login.css" />
  <link rel="stylesheet" href="/public/css/plugins/bootstrap.min.css">
  <% } else if(state == "product") { %>
  <link rel="stylesheet" href="/public/css/product.css">
  <link rel="stylesheet" href="/public/css/plugins/bootstrap.min.css">
  <% } else if(state == "productDetails") { %>
  <link rel="stylesheet" href="/public/css/productDetails.css" />
  <link rel="stylesheet" href="/public/css/index.css">
  <link rel="stylesheet" href="/public/css/plugins/bootstrap.min.css">
  <% } else if(state == "shipping") { %>
  <link rel="stylesheet" href="/public/css/shipping.css" >
  <link rel="stylesheet" href="/public/css/index.css">
  <link rel="stylesheet" href="/public/css/plugins/bootstrap.min.css">
  <link href="https://fonts.googleapis.com/css2?family=Josefin+Sans:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;1,100;1,200;1,300;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet">
  <% } else if(state == "signup") { %>
  <link rel="stylesheet" href="/public/css/signup.css" />
  <link rel="stylesheet" href="/public/css/index.css">
  <link rel="stylesheet" href="/public/css/plugins/bootstrap.min.css">
  <% } else if(state == "T&C") { %>
  <link rel="stylesheet" href="/public/css/T&C.css" >
  <link rel="stylesheet" href="/public/css/plugins/bootstrap.min.css">
  <link href="https://fonts.googleapis.com/css2?family=Josefin+Sans:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;1,100;1,200;1,300;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet">
  <% } else if(state == "wishlist") { %>
  <link rel="stylesheet" href="/public/css/wishlist.css">
  <link rel="stylesheet" href="/public/css/plugins/bootstrap-checkout.min.css">
  <% } %>
</head>

