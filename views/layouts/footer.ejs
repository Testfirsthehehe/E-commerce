<script>
loadMoreBtn.onclick = () => {
  let boxes = [
    ...document.querySelectorAll(".section .product-center .product-item"),
  ];
  for (var i = currentItem; i < currentItem + 8; i++) {
    boxes[i].style.display = "inline-block";
  }
  currentItem += 8;

  if (currentItem >= boxes.length) {
    loadMoreBtn.style.display = "none";
  }
};</script>
